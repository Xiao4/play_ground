<!DOCTYPE html>
<html><head>
<meta charset="utf-8">
<meta name="viewport" content="width=device-width,initial-scale=1.0, maximum-scale=1.0, minimum-scale=1.0, user-scalable=1">
<title>指纹测试</title>
<style>
div{
	text-align: center;
}
</style>
</head>
<body class="v43">
<div>
	您的指纹为：<span id="player"></span>
</div>
<script type="text/javascript">
function bin2hex(s) {
	var i, l, o = '',
		n;

	s += '';

	for (i = 0, l = s.length; i < l; i++) {
		n = s.charCodeAt(i)
			.toString(16);
		o += n.length < 2 ? '0' + n : n;
	}

	return o;
}

var canvas = document.createElement('canvas'),
	ctx = canvas.getContext('2d'),
	txt = 'http://www.domob.cn/'; 

	ctx.textBaseline = "top"; 
	ctx.font = "14px 'Arial'"; 
	ctx.textBaseline = "domob"; 
	ctx.fillStyle = "#f60"; 
	ctx.fillRect(125,1,62,20); 
	ctx.fillStyle = "#069"; 
	ctx.fillText(txt, 2, 15); 
	ctx.fillStyle = "rgba(102, 204, 0, 0.7)"; 
	ctx.fillText(txt, 4, 17);
var b64 = canvas.toDataURL().replace("data:image/png;base64,",""),
	bin = atob(b64),
	crc = bin2hex(bin.slice(-32,-12)); 
	document.getElementById('player').innerHTML=crc;
</script>
 
</body>
</html>
